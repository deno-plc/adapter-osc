{
    "name": "@deno-plc/adapter-osc",
    "version": "1.1.0",
    "exports": "./mod.ts",
    "fmt": {
        "indentWidth": 4
    },
    "tasks": {
        "check": "deno fmt && deno lint && deno check mod.ts && deno test --parallel",
        "check-ci": "deno fmt --check && deno lint && deno check mod.ts && deno test --parallel"
    },
    "lint": {
        "rules": {
            "exclude": [
                "no-inferrable-types"
            ]
        }
    },
    "publish": {
        "exclude": [
            "examples",
            "*.bench.ts",
            "*.test.ts",
            "*.slow.ts"
        ]
    },
    "imports": {
        "@deno-plc/adapter-tcp": "jsr:@deno-plc/adapter-tcp@^1.1.0",
        "@deno-plc/slip": "jsr:@deno-plc/slip@^1.3.0",
        "@std/assert": "jsr:@std/assert@^1.0.11"
    }
}
